<?xml version="1.0" encoding="UTF-8"?>
<entities do-package="com.ctrip.xpipe.redis.console.model" gen="true">
	<entity name="dc-tbl" table="DC_TBL" alias="dt">
		<query-defs>
			<query name="find-all-dcs" type="SELECT" multiple="true">
				<statement>
					<![CDATA[
					SELECT <FIELDS/>
					FROM <TABLE/>
					]]>
				</statement>
			</query>

			<query name="find-dc-by-dc-name" type="SELECT" multiple="false">
				<param name='dc-name'/>

				<statement>
					<![CDATA[
					SELECT <FIELDS/>
					FROM <TABLE/>

					WHERE <FIELD name='dc-name'/> = ${dc-name}
					]]>
				</statement>
			</query>
		</query-defs>
	</entity>

	<entity name='metaserver-tbl' table="METASERVER_TBL" alias="mt">
		<query-defs>
			<query name="find-all-by-dc-id" type="SELECT" multiple="true">
				<param name='dc-id'/>
				<statement>
					<![CDATA[
					SELECT <FIELDS/>
					FROM <TABLE/>
					WHERE <FIELD name='dc-id' /> = ${dc-id}
					]]>					
				</statement>
			</query>

			<query name="find-master-metaserver-by-dc-id" type="SELECT" multiple="false">
				<param name='dc-id'/>
				<statement>
					<![CDATA[
					SELECT <FIELDS/>
					FROM <TABLE/>
					WHERE <FIELD name='dc-id' /> = ${dc-id} 
					AND <FIELD name='metaserver-role' /> = 'master'
					]]>
				</statement>
			</query>
		</query-defs>
	</entity>

