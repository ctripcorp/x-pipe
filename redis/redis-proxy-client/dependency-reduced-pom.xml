                  <include>com.ctrip.framework.xpipe:core</include>
                  <include>org.ow2.asm:asm</include>
                </includes>
              </artifactSet>
              <transformers>
                <transformer>
                  <manifestEntries>
                    <Premain-Class>com.ctrip.framework.xpipe.redis.PreMain</Premain-Class>
                  </manifestEntries>
                </transformer>
              </transformers>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
