<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.ctrip</groupId>
        <artifactId>super-pom</artifactId>
        <version>1.0.4</version>
    </parent>

    <groupId>com.ctrip.framework</groupId>
    <artifactId>framework-bom</artifactId>
    <version>8.9.1</version>
    <packaging>pom</packaging>
    <name>Ctrip Framework BOM</name>
    <description>Ctrip Framework BOM</description>
    <organization>
        <name>Ctrip, Inc.</name>
        <url>http://www.ctrip.com</url>
    </organization>
    <developers>
        <developer>
            <email>rdkjbom@ctrip.com</email>
        </developer>
    </developers>
    <properties>
        <!--ctrip third party bom -->
        <ctrip-thirdparty-bom.version>0.22.0</ctrip-thirdparty-bom.version>
        <!-- framework -->
        <common-toolkit.version>1.0.1</common-toolkit.version>
        <cat.version>3.5.19</cat.version>
        <clogging.version>4.9.0</clogging.version>
        <triplog.version>0.0.63</triplog.version>
        <dal.version>3.0.7</dal.version>
        <ctrip-datasource.version>3.0.7</ctrip-datasource.version>
        <idgen-client.version>1.1.8</idgen-client.version>
        <soa.version>2.24.39</soa.version>
        <artemis.version>1.9.20</artemis.version>
        <caravan.version>1.19.26</caravan.version>
        <framework-foundation.version>1.8.25</framework-foundation.version>
        <cdubbo.version>1.7.37</cdubbo.version>
        <dubbo.version>2.7.7-ctrip.20</dubbo.version>
        <dubbo-serialization.version>2.1.0</dubbo-serialization.version>
        <hystrix.version>1.5.12-ctrip.2</hystrix.version>
        <cmongo-client-sync.version>1.0.1</cmongo-client-sync.version>
        <mongodb-driver.version>4.10.2</mongodb-driver.version>
        <ucs.version>0.7.0</ucs.version>
        <!-- arch bom -->
        <arch-bom.version>2.1.302.1</arch-bom.version>

        <!-- third party -->
        <foundation-service.version>3.0.8</foundation-service.version>
        <plexus-container-default.version>3.0.8</plexus-container-default.version>
        <cs2j.version>1.0.0</cs2j.version>
        <!-- in line with the version used by tars -->
        <tomcat.version>7.0.73</tomcat.version>
        <starter-tomcat.version>7.0.73.ctrip.1</starter-tomcat.version>

        <!-- httpasyncclient -->
        <http.async.client.version>4.1.2-ctrip.1</http.async.client.version>

        <!--jackson-datatype-joda-->
        <jackson.datatype.joda.version>2.8.5</jackson.datatype.joda.version>

        <!-- mysql connector -->
        <mysql-connector.version>8.0.33-ctrip.3</mysql-connector.version>
        
        <!-- mssql connector -->
        <mssql-jdbc.version>6.4.0.jre7.ctrip</mssql-jdbc.version>
        

        <!-- framework bom repository path, only used for framework bom deployment -->
        <!-- Maven Release Repository URL -->
        <releases.repo>http://maven.release.ctripcorp.com/nexus/content/repositories/fxrelease</releases.repo>
        <!-- Maven Snapshot Repository URL -->
        <snapshots.repo>http://maven.release.ctripcorp.com/nexus/content/repositories/fxsnapshot</snapshots.repo>
        <commons-configuration.version>1.8</commons-configuration.version>
    </properties>
    <dependencyManagement>
        <dependencies>
            <!-- Third Party-->
            <dependency>
                <groupId>com.ctrip.thirdparty</groupId>
                <artifactId>ctrip-thirdparty-bom</artifactId>
                <version>${ctrip-thirdparty-bom.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>

            <!-- common-toolkit -->
