                <version>${dubbo.version}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.dubbo</groupId>
                <artifactId>dubbo-metadata-definition-protobuf</artifactId>
                <version>${dubbo.version}</version>
                <exclusions>
                    <exclusion>
                        <groupId>org.apache.dubbo</groupId>
                        <artifactId>dubbo-common</artifactId>
                    </exclusion>
                    <exclusion>
                        <groupId>org.apache.dubbo</groupId>
                        <artifactId>dubbo-metadata-api</artifactId>
                    </exclusion>
                 </exclusions>     
            </dependency>
            <dependency>
                 <groupId>com.ctrip.framework.cdubbo</groupId>
                 <artifactId>cdubbo-serialization</artifactId>
                 <version>${dubbo-serialization.version}</version>
            </dependency>

            <!--Hystrix-core的1.5.12版本依赖了1.8版本的 commons-configuration -->
            <dependency>
                <groupId>commons-configuration</groupId>
                <artifactId>commons-configuration</artifactId>
                <version>${commons-configuration.version}</version>
            </dependency>
            <!--hystrix-->
            <dependency>
             <groupId>com.netflix.hystrix</groupId>
              <artifactId>hystrix-core</artifactId>
              <version>${hystrix.version}</version>
               <!-- conflict with jcl-over-slf4j -->
                <exclusions>
                <exclusion>
                 <groupId>commons-logging</groupId>
                <artifactId>commons-logging</artifactId>
                </exclusion>
                </exclusions>
           </dependency>
            <!-- Cavaran -->
            <dependency>
                <groupId>com.ctrip.soa.caravan</groupId>
